  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "eslint-config-atomic": "^1.18.1",
    "npm-check-updates": "16.10.12",
    "prettier-config-atomic": "^3.0.10",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@babel/cli": "^7",
    "@babel/core": "^7"
  },
  "optionalDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "7.9.7"
  },
  "keywords": [
    "babel",
    "babel-preset"
  ]
